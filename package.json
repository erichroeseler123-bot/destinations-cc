{
  "name": "destinations-cc",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "clean": "rm -rf .next out .turbo",
    "rebuild": "npm run clean && npm run build",
    "deploy": "npm run rebuild && wrangler pages deploy out",
    "add-city": "node scripts/add-city.mjs",
    "add-cities": "node scripts/add-cities.mjs",
    "ingest:viator": "node scripts/viator-ingest.mjs",
    "ingest:daytrips": "node scripts/viator-ingest-daytrips.mjs",
    "ingest:all": "npm run ingest:viator && npm run ingest:daytrips",
    "data:check": "node scripts/validate-data.mjs",
    "data:refresh": "npm run ingest:all && npm run data:check && npm run rebuild",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.4",
    "haversine": "^1.1.1",
    "next": "^16.1.6",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.24",
    "eslint": "8.57.1",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  }
}
